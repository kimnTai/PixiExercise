    "versionDate": "2022-09-20",
    "description": "",
    "main": "index.jsx",
    "gameTitle": "Money Train 3",
    "scripts": {
        "start": "node server.js",
        "build": "cross-env BABEL_ENV=production ./node_modules/.bin/webpack --config webpack.config.production.js",
        "lint": "eslint --cache --ignore-path .gitignore --format=node_modules/eslint-formatter-pretty . *.js",
        "test": "jest",
        "packsound-desktop-stereo": "audiosprite -f howler -g 0 -c 2 -i 1 -l debug -o ./assets/desktop/sounds/soundSpriteStereo ./assetsSource/sounds/stereo/*",
        "packsound-desktop-mono": "audiosprite -f howler -g 0 -c 1 -b 64 -i 1 -l debug -o ./assets/desktop/sounds/soundSpriteMono ./assetsSource/sounds/mono/*",
        "packsound-mobile-mono1": "audiosprite -f howler -g 0 -c 1 -b 64 -i 1 -l debug -o ./assets/mobile/sounds/soundSpriteMono1 ./assetsSource/sounds/stereo/*",
        "packsound-mobile-mono2": "audiosprite -f howler -g 0 -c 1 -b 64 -i 1 -l debug -o ./assets/mobile/sounds/soundSpriteMono2 ./assetsSource/sounds/mono/*",
        "pack-all-sounds": "yarn packsound-desktop-stereo && yarn packsound-desktop-mono && yarn packsound-mobile-mono1 && yarn packsound-mobile-mono2"
    },
    "jest": {
        "globals": {
            "PIXI": {},
            "React": "node_modules/react"
        },
        "moduleNameMapper": {
            "pixi.js": "<rootDir>/test/pixi-test.js",
            "pixi-spine": "<rootDir>/test/pixi-test.js",
            "config": "<rootDir>/test/config-test.js",
            "macguffin": "<rootDir>/test/macguffin-test.js"
        },
        "setupFiles": [
            "<rootDir>/test/jest.setup.js"
        ],
        "transformIgnorePatterns": [
            "/node_modules(?![\\|/](macguffin|mc_))/"
        ]
    },
    "keywords": [
        "react",
        "es6",
        "babel",
        "webpack"
    ],
    "author": "Relax-Gaming",
